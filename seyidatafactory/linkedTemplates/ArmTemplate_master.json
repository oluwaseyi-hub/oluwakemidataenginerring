{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "seyidatafactory"
		},
		"LS_ADLSproject3_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSproject3'"
		},
		"LS_fornewADLSstorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_fornewADLSstorage'"
		},
		"LS_sqldatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_sqldatabase'"
		},
		"ls_adlsstcopy_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adlsstcopy'"
		},
		"ls_blobstcopy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blobstcopy'"
		},
		"LS_ADLSproject3_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsforproject3.dfs.core.windows.net/"
		},
		"LS_fornewADLSstorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsforproject02.dfs.core.windows.net/"
		},
		"LS_sqldatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverforproject.database.windows.net"
		},
		"LS_sqldatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DB_forkemproject"
		},
		"LS_sqldatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "oluwaseyiadmin"
		},
		"ls_adlsstcopy_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsforproject.dfs.core.windows.net/"
		},
		"TR_dessertforall_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/81141c92-9125-49b0-87a3-ab9093ff889b/resourceGroups/RG-oluwaseyi/providers/Microsoft.Storage/storageAccounts/adlsforproject3"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "seyidatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSproject3_accountKey": {
						"value": "[parameters('LS_ADLSproject3_accountKey')]"
					},
					"LS_fornewADLSstorage_accountKey": {
						"value": "[parameters('LS_fornewADLSstorage_accountKey')]"
					},
					"LS_sqldatabase_password": {
						"value": "[parameters('LS_sqldatabase_password')]"
					},
					"ls_adlsstcopy_accountKey": {
						"value": "[parameters('ls_adlsstcopy_accountKey')]"
					},
					"ls_blobstcopy_connectionString": {
						"value": "[parameters('ls_blobstcopy_connectionString')]"
					},
					"LS_ADLSproject3_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSproject3_properties_typeProperties_url')]"
					},
					"LS_fornewADLSstorage_properties_typeProperties_url": {
						"value": "[parameters('LS_fornewADLSstorage_properties_typeProperties_url')]"
					},
					"LS_sqldatabase_properties_typeProperties_server": {
						"value": "[parameters('LS_sqldatabase_properties_typeProperties_server')]"
					},
					"LS_sqldatabase_properties_typeProperties_database": {
						"value": "[parameters('LS_sqldatabase_properties_typeProperties_database')]"
					},
					"LS_sqldatabase_properties_typeProperties_userName": {
						"value": "[parameters('LS_sqldatabase_properties_typeProperties_userName')]"
					},
					"ls_adlsstcopy_properties_typeProperties_url": {
						"value": "[parameters('ls_adlsstcopy_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "seyidatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"TR_dessertforall_properties_typeProperties_scope": {
						"value": "[parameters('TR_dessertforall_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'seyidatafactory_ArmTemplate_0')]"
			]
		}
	]
}