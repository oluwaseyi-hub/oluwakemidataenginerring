{
	"name": "pl_for_store_analytics",
	"properties": {
		"description": "To copy data from Json in git hub to ADLS, and Azure SQL database to ADLS.",
		"activities": [
			{
				"name": "Copy data from Json in git to ADLS",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_for_storeanalytis_Json",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_storeAnalytics_Parquet",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}